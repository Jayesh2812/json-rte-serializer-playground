{
  "name": "json-rte-serializer-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "postinstall": "npm run extract-types",
    "extract-types": "node scripts/extractTypes.js",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "connect:local": "npm install ../new-json-rte-serializer",
    "connect:remote": "npm uninstall @contentstack/json-rte-serializer && npm install @contentstack/json-rte-serializer"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@contentstack/app-sdk": "^2.1.0",
    "@contentstack/json-rte-serializer": "^3.0.3",
    "@contentstack/utils": "^1.3.8",
    "@monaco-editor/react": "^4.6.0",
    "antd": "^5.27.0",
    "emmet-monaco-es": "^5.5.0",
    "javascript-stringify": "^2.1.0",
    "jsonrepair": "^3.13.0",
    "lodash.get": "^4.4.2",
    "monaco-editor": "^0.52.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/lodash.get": "^4.4.9",
    "@types/pretty": "^2.0.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "collapse-whitespace": "^1.1.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "pre-push": "^0.1.4",
    "pretty": "^2.0.0",
    "typescript": "^5.9.2",
    "vite": "^5.3.1"
  },
  "pre-push": [
    "build"
  ]
}
